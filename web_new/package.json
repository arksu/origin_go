{
  "name": "origin-web-client",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npm run proto && vite",
    "build": "npm run proto && vue-tsc --build && vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "proto": "pbjs -t static-module -w es6 -o src/network/proto/packets.js ../api/proto/packets.proto && pbts -o src/network/proto/packets.d.ts src/network/proto/packets.js"
  },
  "dependencies": {
    "@esotericsoftware/spine-pixi-v8": "^4.2.101",
    "axios": "^1.13.4",
    "howler": "^2.2.4",
    "pinia": "^3.0.4",
    "pixi.js": "^8.16.0",
    "protobufjs": "^7.5.4",
    "vue": "^3.5.27",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.15.0",
    "@tsconfig/node20": "^20.1.9",
    "@types/howler": "^2.2.12",
    "@types/node": "^25.2.1",
    "@vitejs/plugin-vue": "^6.0.4",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.39.2",
    "eslint-plugin-vue": "^9.33.0",
    "protobufjs-cli": "^2.0.0",
    "sass": "^1.97.3",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vue-tsc": "^3.2.4"
  }
}
